{
  "name": "gulp-project",
  "version": "2.0.0",
  "description": "Gulp Project",
  "main": "",
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "start": "gulp watch",
    "commit": "git cz",
    "lint:css": "stylelint ./src/**/*.scss",
    "lint:js": "standard ./src/**/*.js",
    "lint:fix": "stylelint --fix ./src/**/*.scss && standard --fix ./src/**/*.mjs",
    "test:visual": "backstop test --config=tools/test-runners/backstop/main-config.mjs",
    "test:visual-approve": "backstop approve --config=tools/test-runners/backstop/main-config.mjs",
    "test:e2e": "gulp e2e-test --silent",
    "test": "npm run test:visual && npm run test:e2e --silent",
    "build": "gulp build --production",
    "dev:clean": "gulp clean:dev",
    "build:clean": "gulp clean",
    "deploy": "gulp deploy",
    "prepare": "husky"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tomasvn/gulp-project.git"
  },
  "author": "Ngoc Tu Nguyen",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/tomasvn/gulp-project/issues"
  },
  "homepage": "https://github.com/tomasvn/gulp-project#readme",
  "devDependencies": {
    "@babel/preset-env": "^7.25.4",
    "@yimura/import-dir": "^0.1.12",
    "browser-sync": "^2.29.3",
    "del": "^7.1.0",
    "git-cz": "^4.9.0",
    "gulp": "^5.0.0",
    "gulp-autoprefixer": "^9.0.0",
    "gulp-babel": "^8.0.0",
    "gulp-concat": "^2.6.1",
    "gulp-config-strip-debug": "^1.0.0",
    "gulp-cssnano": "^2.1.3",
    "gulp-if": "^3.0.0",
    "gulp-imagemin": "^9.1.0",
    "gulp-nightwatch": "^1.2.0",
    "gulp-plumber": "^1.2.1",
    "gulp-sass": "^5.1.0",
    "gulp-size": "^5.0.0",
    "gulp-sourcemaps": "^3.0.0",
    "gulp-surge": "^0.1.4",
    "gulp-uglify": "^3.0.2",
    "gulp-useref": "^4.0.0",
    "husky": "^9.1.5",
    "lint-staged": "^15.2.9",
    "nightwatch": "^1.7.13",
    "prettier": "^3.3.3",
    "run-sequence": "^2.2.1",
    "selenium-server-standalone-jar": "^4.22.0",
    "standard": "^17.1.0",
    "stylelint": "^16.9.0",
    "stylelint-config-standard": "^36.0.1"
  },
  "dependencies": {
    "chromedriver": "^128.0.1",
    "pump": "^3.0.0",
    "sass": "^1.77.8"
  },
  "config": {
    "commitizen": {
      "path": "git-cz"
    }
  }
}
