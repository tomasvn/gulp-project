{
  "name": "gulp-project",
  "version": "1.2.1",
  "description": "Gulp Project",
  "main": "index.js",
  "scripts": {
    "start": "gulp watch",
    "commit": "git cz",
    "lint:css": "stylelint ./src/**/*.scss",
    "lint:js": "standard ./src/**/*.js",
    "lint:fix": "stylelint --fix ./src/**/*.scss && standard --fix ./src/**/*.js",
    "test:visual": "backstop test --config=tools/test-runners/backstop/mainConfig.js",
    "test:visual-approve": "backstop approve --config=tools/test-runners/backstop/mainConfig.js",
    "test:e2e": "gulp e2e-test --silent",
    "test": "npm run test:visual && npm run test:e2e --silent",
    "build": "gulp build",
    "dev:clean": "gulp clean:dev",
    "build:clean": "gulp clean",
    "deploy": "gulp deploy"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": ""
    }
  },
  "lint-staged": {
    "./src/**/*.js": [
      "prettier --write",
      "npm test",
      "npm run lint:js",
      "git add -A"
    ],
    "./src/**/*.scss": [
      "npm run lint:css",
      "git add -A"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tomasvn/gulp-project.git"
  },
  "author": "Ngoc Tu Nguyen",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/tomasvn/gulp-project/issues"
  },
  "homepage": "https://github.com/tomasvn/gulp-project#readme",
  "devDependencies": {
    "@babel/preset-env": "^7.7.7",
    "browser-sync": "^2.26.7",
    "del": "^5.1.0",
    "git-cz": "^3.3.0",
    "gulp": "^3.9.1",
    "gulp-autoprefixer": "^7.0.1",
    "gulp-babel": "^8.0.0",
    "gulp-concat": "^2.6.1",
    "gulp-config-strip-debug": "^1.0.0",
    "gulp-cssnano": "^2.1.3",
    "gulp-if": "^3.0.0",
    "gulp-imagemin": "^6.0.0",
    "gulp-nightwatch": "^1.0.1",
    "gulp-plumber": "^1.2.1",
    "gulp-sass": "^4.0.2",
    "gulp-size": "^3.0.0",
    "gulp-sourcemaps": "^2.6.5",
    "gulp-surge": "^0.1.3",
    "gulp-uglify": "^3.0.2",
    "gulp-useref": "^4.0.0",
    "husky": "^3.1.0",
    "lint-staged": "^9.5.0",
    "nightwatch": "^1.3.2",
    "prettier": "^1.19.1",
    "require-dir": "^1.2.0",
    "run-sequence": "^2.2.1",
    "selenium-server-standalone-jar": "^3.141.5",
    "standard": "^14.3.1",
    "stylelint": "^15.10.1",
    "stylelint-config-standard": "^19.0.0"
  },
  "dependencies": {
    "chromedriver": "^75.0.1",
    "pump": "^3.0.0"
  },
  "config": {
    "commitizen": {
      "path": "git-cz"
    }
  }
}
